{
    "builds": [
        {
            "src": "call-dashboard-app/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "skypulsebot.vercel.app"
                }
            ],
            "destination": "https://skypulsebot-five.vercel.app/$1",
            "permanent": true
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/call-dashboard-app/$1"
        }
    ]
}