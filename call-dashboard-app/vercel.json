{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/vapi-api/(.*)",
            "dest": "https://api.vapi.ai/$1"
        }
    ]
}